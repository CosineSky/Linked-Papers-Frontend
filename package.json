{
  "name": "2024-devops-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "axios": "^1.7.7",
    "core-js": "^3.38.1",
    "element-plus": "^2.8.6",
    "swiper": "^11.1.14",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-modules-commonjs": "^7.25.9",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "AMap": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "comma-dangle": 0,
      "no-cond-assign": 0,
      "no-console": 0,
      "no-constant-condition": 0,
      "no-control-regex": 0,
      "no-debugger": 0,
      "no-dupe-args": 0,
      "no-dupe-keys": 0,
      "no-duplicate-case": 0,
      "no-empty": 0,
      "no-empty-character-class": 0,
      "no-ex-assign": 0,
      "no-extra-boolean-cast": 0,
      "no-extra-semi": 0,
      "no-func-assign": 0,
      "no-inner-declarations": ["error", "functions"],
      "no-invalid-regexp": 0,
      "no-irregular-whitespace": 0,
      "no-negated-in-lhs": 0,
      "no-obj-calls": 0,
      "no-regex-spaces": 0,
      "no-sparse-arrays": 0,
      "no-unreachable": 0,
      "use-isnan": 0,
      "valid-jsdoc": ["error", {
        "requireReturn": false,
        "requireParamDescription": false,
        "requireReturnDescription": true
      }],
      "valid-typeof": ["error", {
        "requireStringLiterals": true
      }],
      "block-scoped-var": 0,
      "complexity": 0,
      "consistent-return": 0,
      "curly": 0,
      "default-case": 0,
      "dot-notation": ["error", {"allowKeywords": false, "allowPattern": ""}],
      "eqeqeq": ["error", "smart"],
      "no-alert": 1,
      "no-caller": 0,
      "guard-for-in": 0,
      "no-div-regex": 0,
      "no-else-return": 0,
      "no-labels": ["error", {
        "allowLoop": false,
        "allowSwitch": false
      }],
      "no-eq-null": 0,
      "no-eval": 0,
      "no-extend-native": 0,
      "no-extra-bind": 0,
      "no-fallthrough": 0,
      "no-floating-decimal": 0,
      "no-implied-eval": 0,
      "no-iterator": 0,
      "no-lone-blocks": 0,
      "no-loop-func": 0,
      "no-multi-spaces": 0,
      "no-multi-str": 0,
      "no-global-assign": 0,
      "no-new": 0,
      "no-new-func": 0,
      "no-new-wrappers": 0,
      "no-octal": 0,
      "no-octal-escape": 0,
      "no-param-reassign": 0,
      "no-proto": 0,
      "no-redeclare": 0,
      "no-return-assign": 0,
      "no-script-url": 0,
      "no-self-compare": 0,
      "no-sequences": 0,
      "no-throw-literal": 0,
      "no-unused-expressions": 0,
      "no-void": 0,
      "no-warning-comments": [1, {"terms": ["todo", "fixme", "any other term"]}],
      "no-with": 0,
      "radix": 1,
      "vars-on-top": 0,
      "wrap-iife": [0, "any"],
      "yoda": [0, "never", {"exceptRange": true}],
      "strict": [0, "function"],
      "no-catch-shadow": 0,
      "no-delete-var": 0,
      "no-label-var": 0,
      "no-shadow": 0,
      "no-shadow-restricted-names": 0,
      "no-undef": 0,
      "no-undef-init": 0,
      "no-undefined": 0,
      "no-unused-vars": [0, {"vars": "all", "args": "after-used"}],
      "no-use-before-define": [0, "nofunc"],
      "indent": 0,
      "brace-style": [0, "1tbs", { "allowSingleLine": false}],
      "camelcase": [0, {"properties": "never"}],
      "comma-style": [0, "last"],
      "consistent-this": [0, "self"],
      "eol-last": 0,
      "func-names": 0,
      "func-style": 0,
      "key-spacing": [0, {"beforeColon": false, "afterColon": true}],
      "max-nested-callbacks": 0,
      "new-cap": [0, {"newIsCap": true, "capIsNew": false}],
      "new-parens": 0,
      "newline-after-var": 0,
      "no-array-constructor": 0,
      "no-inline-comments": 0,
      "no-lonely-if": 0,
      "no-mixed-spaces-and-tabs": [0, "smart-tabs"],
      "no-multiple-empty-lines": [0, {"max": 0}],
      "no-nested-ternary": 0,
      "no-new-object": 0,
      "fun-call-spacing": 0,
      "no-ternary": 0,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "no-extra-parens": 0,
      "one-var": 0,
      "operator-assignment": 0,
      "padded-blocks": [0, "never"],
      "quote-props": 0,
      "quotes": [1, "single", "avoid-escape"],
      "semi": [0, "always"],
      "semi-spacing": [0, {"before": false, "after": true}],
      "sort-vars": 0,
      "space-before-blocks": [0, "always"],
      "space-before-function-paren": [0, {"anonymous": "always", "named": "never"}],
      "space-infix-ops": [0, {"int30Hint": true}],
      "keyword-spacing": 0,
      "space-unary-ops": [0, { "words": true, "nonwords": false}],
      "wrap-regex": 0,
      "no-var": 0,
      "generator-star-spacing": [0, "both"],
      "max-depth": 0,
      "max-len": 0,
      "max-params": 0,
      "max-statements": 0,
      "no-bitwise": 0,
      "no-plusplus": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
